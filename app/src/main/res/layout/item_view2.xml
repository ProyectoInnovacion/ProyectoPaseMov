<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/rounded_comer"
    android:layout_marginBottom="10dp">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tituloOutput2"
        android:textSize="20dp"
        android:textColor="@color/black"
        android:text="Mi titulo"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/descripcionOutput2"
        android:textSize="20dp"
        android:layout_below="@+id/tituloOutput2"
        android:textColor="#393939"
        android:text="Mi Descripcion"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/timeOutput2"
        android:textSize="12dp"
        android:layout_below="@+id/descripcionOutput2"
        android:textColor="#858484"
        android:gravity="end"
        android:text="jan 21 2020"
        />

</RelativeLayout>